<?xml version="1.0" encoding="UTF-8"?>
<schema
  xmlns="http://www.concrete5.org/doctrine-xml/0.5"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 http://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd"
>

  <table name="btTallacmansCv">
    <field name="bID" type="integer">
      <unsigned/>
      <key/>
    </field>
  </table>

  <table name="btTallacmansCvCvEntries">
    <field name="id" type="integer">
      <unsigned/>
      <autoincrement/>
      <key/>
    </field>
    <field name="bID" type="integer">
      <unsigned/>
    </field>
    <field name="sortOrder" type="integer"/>
    <field name="position" type="string" size="255"/>
    <field name="employer" type="string" size="255"/>
    <field name="summary" type="text"/>
    <field name="dateStart" type="integer"/>
    <field name="dateEnd" type="integer"/>
  </table>

</schema>
